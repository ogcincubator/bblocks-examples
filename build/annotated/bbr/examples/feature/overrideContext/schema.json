{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Example of a simple GeoJSON Feature specialisation",
  "$defs": {
    "MyFeature": {
      "allOf": [
        {
          "$ref": "https://ogcincubator.github.io/bblocks-examples/build/annotated/bbr/examples/feature/externalSchema/schema.json"
        }
      ]
    }
  },
  "anyOf": [
    {
      "$ref": "#/$defs/MyFeature"
    }
  ],
  "x-jsonld-extra-terms": {
    "Name": "rdfs:label"
  }
}